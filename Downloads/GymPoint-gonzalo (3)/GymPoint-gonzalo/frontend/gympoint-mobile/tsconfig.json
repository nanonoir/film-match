{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@app/*": ["app/*"],
      "@features/*": ["src/features/*"],
      "@presentation/*": ["src/presentation/*"],
      "@shared/*": ["src/shared/*"],
      "@assets/*": ["assets/*"],
      "@di/*": ["src/di/*"]
    },
    "skipLibCheck": true
  },
  "include": ["app", "src", "App.tsx", "src/types/**/*.d.ts"]
}
