# ============================================
# PRODUCCIÓN - Variables de entorno para deploy
# ============================================

# Backend API Configuration (Actualizar con URL de producción)
VITE_API_BASE_URL=https://your-backend-production-url.com/api
VITE_API_TIMEOUT=30000

# Google OAuth Configuration (Actualizar con credenciales de producción)
VITE_GOOGLE_CLIENT_ID=YOUR_PRODUCTION_GOOGLE_CLIENT_ID
VITE_GOOGLE_REDIRECT_URI=https://your-app-production-url.com/auth/callback

# Feature Flags
VITE_ENABLE_RAG_CHAT=true
VITE_ENABLE_SEMANTIC_SEARCH=true

# Cache Configuration (Más agresivo en producción)
VITE_CACHE_TTL=600000
VITE_CACHE_MAX_SIZE=200

# Logging (Desactivado en producción)
VITE_DEBUG_MODE=false
